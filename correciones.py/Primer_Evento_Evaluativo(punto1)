]



matriz = [[(i * m) + j + 1 for j in range(m)] for i in range(n)]

for i in range(n):
    if i % 2 == 0:
        
        for valor in matriz[i]:
            print(valor, end=' ')
    else:
        
        for valor in reversed(matriz[i]):
            print(valor, end=' ')
