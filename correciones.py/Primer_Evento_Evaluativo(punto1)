n = int(input("Ingrese el número de filas: "))
m = int(input("Ingrese el número de columnas: "))


matriz = [[(i * m) + j + 1 for j in range(m)] for i in range(n)]
print(matriz)
for fila in matriz:
    print(fila)
'''''
for j in range(m):
    if j / 2 == 0:
        for valor in matriz[j]:
            print(valor, end=' ')
    else:
        for valor in reversed(matriz[j]):
            print(valor, end=' ')
'''''
for j in range(m):
  for i in range(n):
    print(matriz[i][j], end=' ')
    if j % 2 == 0:
      for i in range(n):
        print(matriz[i][j], end=' ')
    else:
      for i in range(n):
        print(matriz[i][j], end=' ')

    